<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="293.0" prefWidth="368.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.configuratoreauto.Controllers.RegistrazioneController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Registrazione">
        <font>
            <Font size="21.0" />
        </font>
    </Label>
   <TextField fx:id="nome" promptText="Nome" />
   <TextField fx:id="cognome" promptText="Cognome" />
    <TextField fx:id="email" promptText="Email" />
    <PasswordField fx:id="password" promptText="Password" />
   <PasswordField fx:id="passwordConferma" prefWidth="328.0" promptText="Conferma la Password" />
    <ButtonBar prefHeight="40.0" prefWidth="200.0" />
      <Button fx:id="registraButton" onAction="#registraUtente" text="Registrati" textAlignment="CENTER" />
    <Label fx:id="responseText">
    </Label>
</VBox>
